<% if current_user.admin  %>
  <h1>ADMIN</h1>
  <ul>

    <% @users.each do |user|  %>
      <li ><%= user.email %>
        <select id="<%="#{user.id}"%>" class="user" name:"user">
          <option value="">select user</option>
          <option value="true">admin</option>
          <option value="false">student</option>
        </select>
        <p><%= user.admin %></p>
      </li>



    <% end %>
  </ul>
<% else %>
  <h1>No acces</h1>
<% end %>
